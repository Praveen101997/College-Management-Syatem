<AnchorPane id="AnchorPane" fx:id="apane2" prefHeight="363.0" prefWidth="418.0" stylesheets="@../login/dark-theme.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="student_portal.ui.register.RegisterController">
   <children>
      <ImageView fitHeight="65.0" fitWidth="67.0" layoutX="176.0" layoutY="20.0">
         <image>
            <Image url="@register.jfif" />
         </image>
      </ImageView>
      <JFXTextField fx:id="userid" labelFloat="true" layoutX="29.0" layoutY="112.0" promptText="Enter UserName" />
      <JFXTextField fx:id="emailid" labelFloat="true" layoutX="228.0" layoutY="112.0" promptText="Enter Email Id" />
      <JFXPasswordField fx:id="password" labelFloat="true" layoutX="29.0" layoutY="189.0" promptText="Enter Password" />
      <JFXPasswordField fx:id="repassword" labelFloat="true" layoutX="228.0" layoutY="189.0" promptText="ReEnter Password" />
      <JFXButton layoutX="104.0" layoutY="303.0" onAction="#signupformsubmit" prefHeight="40.0" prefWidth="90.0" text="SignUp" />
      <Button layoutX="228.0" layoutY="303.0" mnemonicParsing="false" onAction="#exitregisterform" prefHeight="40.0" prefWidth="90.0" text="Cancel" />
      <Label fx:id="errortext" layoutX="119.0" layoutY="340.0" prefHeight="17.0" prefWidth="173.0" textFill="WHITE">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <JFXTextField fx:id="currentyear" alignment="CENTER" labelFloat="true" layoutX="176.0" layoutY="243.0" onAction="#signupformsubmit" prefHeight="37.0" prefWidth="60.0" promptText="Year" />
   </children>
</AnchorPane>
